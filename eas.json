{
  "cli": {
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true,
        "buildConfiguration": "Debug"
      },
      "env": {
        "ENVIRONMENT": "development"
      }
    },
    "production": {
      "ios": {
        "simulator": false,
        "enterpriseProvisioning": "universal"
      }
    },
    "preview": {
      "distribution": "internal"
    }
  }
}